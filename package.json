{
  "name": "mini",
  "version": "1.0.0",
  "private": true,
  "description": "小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "start": "npm run dev:weapp",
    "start:test": "npm run dev:weapp:test",
    "start:prod": "npm run dev:weapp:prod",
    "build:weapp": "taro build --type weapp",
    "build:weapp:dev": "cross-env PROJECT_ENV=dev taro build --type weapp",
    "build:weapp:test": "cross-env PROJECT_ENV=test taro build --type weapp",
    "build:weapp:prod": "cross-env PROJECT_ENV=prod taro build --type weapp",
